{
    "version": 2,
    "builds": [
      {
        "src": "bin/www",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/bin/www"
      }
    ],
    "env": {
      "MONGO_URL_DEV": "@mongodb_dev_url",
      "MONGO_URL": "@mongodb_prod_url",
      "SECRET": "@secret_key"
    }
  }
  